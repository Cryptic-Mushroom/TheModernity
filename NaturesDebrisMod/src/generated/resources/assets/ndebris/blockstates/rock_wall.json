{
  "multipart": [
    {
      "when": {
        "up": "true"
      },
      "apply": {
        "model": "ndebris:block/rock_wall_post"
      }
    },
    {
      "when": {
        "north": "true"
      },
      "apply": {
        "model": "ndebris:block/rock_wall_side"
      }
    },
    {
      "when": {
        "east": "true"
      },
      "apply": {
        "model": "ndebris:block/rock_wall_side",
        "y": 90
      }
    },
    {
      "when": {
        "south": "true"
      },
      "apply": {
        "model": "ndebris:block/rock_wall_side",
        "y": 180
      }
    },
    {
      "when": {
        "west": "true"
      },
      "apply": {
        "model": "ndebris:block/rock_wall_side",
        "y": 270
      }
    }
  ]
}